{
  "name": "flatworld",
  "description": "Game engine for 2D turn-based strategy games in browsers",
  "keywords": [
    "2D game engine",
    "game-engine",
    "game engine",
    "HTML5 game engine",
    "turn-based strategy game",
    "turn-based",
    "multiplayer",
    "singleplayer",
    "cooperative",
    "strategy",
    "game",
    "webGL",
    "canvas",
    "HTML5"
  ],
  "engines": {
    "node": ">=10.8.0"
  },
  "version": "0.6.5",
  "browser": "dist/flatworld.min.js",
  "author": "Janne Hyyti√§",
  "license": "mit",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hachitus/flatworld.git"
  },
  "bugs": {
    "url": "https://github.com/Hachitus/flatworld/issues"
  },
  "homepage": "https://github.com/Hachitus/flatworld",
  "scripts": {
    "build:dev": "npx webpack --config webpack.config.dev.js",
    "start": "npm run start:dev",
    "start:dev": "npm run build:dev && npx webpack-dev-server --config webpack.config.dev.js",
    "build": "npm run build:prod",
    "build:prod": "npx webpack --config webpack.config.prod.js",
    "start:prod": "npm run build:prod && npx webpack-dev-server --config webpack.config.prod.js",
    "nextVersion": "echo 'MANUAL: npm version patch/minor/major && git tag -a ?.?.? -m 'comment' && git push --tags && update CHANGELOG && update roadmap if needed'",
    "document": "npx yuidoc -t node_modules/yuidoc-theme-blue && gh-pages -d documentation",
    "showDocuments": "echo 'starting instance in http://localhost:9002/' && npx forever start startDocument.js --uid flatworldDocument",
    "test": "npx karma start karma.conf.js --log-level debug --single-run",
    "init": "ln -s /usr/bin/nodejs /usr/bin/node && npm install",
    "lint": "npx eslint ./src/",
    "prepublishOnly": "npm run build:prod",
    "pack:dev": "npm run build & npm pack"
  },
  "devDependencies": {
    "eslint": "^5.11.0",
    "eslint-loader": "^2.1.1",
    "express": "^4.12.4",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^3.0.5",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.13",
    "yuidocjs": "^0.10.0"
  },
  "dependencies": {
    "edge-launcher": "^1.2.2",
    "esm": "^3.0.84",
    "eventemitter3": "^2.0.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "handlebars": "^4.0.12",
    "handlebars-loader": "^1.4.0",
    "howler": "^2.0.1",
    "javascript-state-machine": "^2.3.5",
    "loglevel": "^1.4.1",
    "nvm": "0.0.4",
    "pixi.js": "^4.8.4",
    "whatwg-fetch": "^2.0.4"
  }
}
